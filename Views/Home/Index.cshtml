@{
    ViewBag.Title = "Instant Tutors";
}

<section id="slider" class="carousel slide sliderSec wow fadeInUp animated" data-ride="carousel" data-wow-duration="1s">
    <ul class="carousel-indicators">
        <li data-target="#slider" data-slide-to="0" class="active"></li>
        <li data-target="#slider" data-slide-to="1"></li>
        <li data-target="#slider" data-slide-to="2"></li>
    </ul>
    <div class="carousel-inner">
        <div class="carousel-item active">
            <div class="row">
                <div class="col-6 align-self-center">
                    <div class="slider-content">
                        <h4>ZOOM Meetings Wherever, Whenever </h4>
                        <p>We offer FREE online tutoring services, and students have the luxury of accessing their classes wherever and whenever. Classes will be taught by extremely qualified tutors on ZOOM</p>
                        <a href="Home/About">Read More</a>
                    </div>
                </div>
                <div class="col-6">
                    <div class="slider-img text-right">
                        <img src="~/Content/images/zoom-meeting.png" alt="slide">
                    </div>
                </div>
            </div>
        </div>
        <div class="carousel-item">
            <div class="row">
                <div class="col-6 align-self-center">
                    <div class="slider-content">
                        <h4>Train your memory on complex topics</h4>
                        <p>All Tutoring Sessions will be conducted by extremely qualified high school students on ZOOM. Zoom sessions can go up to 50 min. Students can bring their own material/questions to each Zoom session. We require that you sign up for Zoom Sessions 48 hours in advance.</p>
                        <a href="@Url.Action("Student", "Account", new { area = "" })" hidefocus="hidefocus">Become a Student</a>
                    </div>
                </div>
                <div class="col-6">
                    <div class="slider-img text-right">
                        <img src="~/Content/images/slide_2.png" alt="slide">
                    </div>
                </div>
            </div>
        </div>
        <div class="carousel-item">
            <div class="row">
                <div class="col-6 align-self-center">
                    <div class="slider-content">
                        <h4>Become online professional tutor</h4>
                        <p>Our online instant tutoring platform makes connecting with students or parents simple, convenient and flexible. Aimed to assist students in grades K-8, we guarantee that your student will be paired with the most qualified tutor, according to what they are in need of. Please JOIN US</p>
                        <a href="@Url.Action("Tutor", "Account", new { area = "" })" hidefocus="hidefocus">Become a Tutor</a>
                    </div>
                </div>
                <div class="col-6">
                    <div class="slider-img text-right">
                        <img src="~/Content/images/slide_3.png" alt="slide">
                    </div>
                </div>
            </div>
        </div>
    </div>
    <a class="carousel-control-prev" href="#slider" data-slide="prev">
        <span class="carousel-control-prev-icon"></span>
    </a>
    <a class="carousel-control-next" href="#slider" data-slide="next">
        <span class="carousel-control-next-icon"></span>
    </a>
</section>
<section class="welcome-sec wow fadeInUp animated" data-wow-duration="2s">
    <div class="container">
        <div class="row">
            <div class="col-12 text-center">
                <h1><img src="~/Content/images/icon2.png" alt="icon2"></h1>
                <h3 class="head_title">Welcome to Instant Tutors</h3>
                <h4 class="head_sub_title">Wherever, Whenever | Free Online Tutoring </h4>
                <p class="title-para">As part of the many preventative measures that were taken to stop the spread of COVID-19, 2.6 Floridian students were taken out of school. Instant Tutors is a group of determined highschool students that want to give back to their community, and help students embark on their educational paths. We offer FREE online tutoring services, and students have the luxury of accessing their classes wherever and whenever. </p>
            </div>
        </div>
        <div class="row mt-4">
            <div class="col-12 col-md-6 col-lg-3 text-center">
                <div class="circle-sec">
                    <img src="~/Content/images/online-icon-screen.png" alt="K-5th & 6th-8th">
                    <h3>
                        K-5th & 6th-8th
                    </h3>
                    <p>We assist with all core subjects</p>
                </div>
            </div>
            <div class="col-12 col-md-6 col-lg-3 text-center">
                <div class="circle-sec">
                    <img src="~/Content/images/online-icon-cal.png" alt="FSA/EOC">
                    <h3>FSA/EOC</h3>
                    <p>We tutor for all subjects</p>
                </div>
            </div>
            <div class="col-12 col-md-6 col-lg-3 text-center">
                <div class="circle-sec">
                    <img src="~/Content/images/online-icon-world.png" alt="World Languages">
                    <h3>World Languages</h3>
                    <p>We help with Spanish</p>
                </div>
            </div>
            <div class="col-12 col-md-6 col-lg-3 text-center">
                <div class="circle-sec">
                    <img src="~/Content/images/online-icon-teach.png" alt="Diagnostic Exams">
                    <h3>Diagnostic Exams</h3>
                    <p>If you are interested in seeing where your child is placed academically, contact us for a FREE diagnostic exam.</p>
                </div>
            </div>
        </div>
    </div>
</section>
<section class="join-tutor">
    <div class="container">
        <div class="col-12 col-lg-10 col-xl-8 m-auto">
            <div class="row">
                <div class="col-12 col-md-6 mb-4 mb-md-0">
                    <div class="card">
                        <h5 class="text-center">Join as a</h5>
                        <h2 class="text-center">Student</h2>
                        <ul class="list-unstyled mb-0">
                            <li>
                                Search for online and local tutors using our search service
                            </li>
                            <li>
                                Communicate with any of our tutors for free
                            </li>
                            <li>
                                Classes will be taught by extremely qualified tutors
                            </li>
                            <li>
                                Classes will be conducted on ZOOM & can go upto 60 minutes
                            </li>
                        </ul>
                        <div class="joinbtn text-center"><button class="btn"><a class="" style="color:white" href="@Url.Action("Student", "Account", new { area = "" })">Register as Student</a></button></div>
                    </div>
                </div>
                <div class="col-12 col-md-6">
                    <div class="card tutor-card">
                        <h5 class="text-center text-white">Join as a</h5>
                        <h2 class="text-center text-white">Tutor</h2>
                        <ul class="list-unstyled mb-0">
                            <li>
                                Our site is completely free for tutors & Interactive ZOOM meeting
                            </li>
                            <li>
                                Create your own profile and set your availability
                            </li>
                            <li>
                                Search for tuition volunteering
                            </li>
                            <li>
                                Tutors will receive volunteer hours for tutoring
                            </li>
                        </ul>
                        <div class="joinbtn text-center"><button class="btn"> <a class="" href="@Url.Action("Tutor", "Account", new { area = "" })">Become a Tutor</a></button></div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
<section class="testimonals-sec">
    <div class="container">
        <h3 class="text-center">Parents/Students Testimonials</h3>
        <div id="testimonals" class="carousel slide" data-ride="carousel">
            <div class="carousel-inner text-center">
                <div class="carousel-item active">
                    @*<img src="~/Content/images/Alissa-Nutting.jpg" alt="Alissa Nutting" />*@
                    <p>I used to struggle with maths. After getting a home tutor I am doing maths very easily and topped my class.</p>
                    <h5> - Alissa Nutting</h5>
                </div>
                <div class="carousel-item">
                    @*<img src="~/Content/images/Claire-Contreras.jpg" alt="Claire Contreras" />*@
                    <p>Earlier I am too worried about my kids addiction to Mobile & TV. Now when home tutor comes home to teach they study perfectly.</p>
                    <h5> - Isabella</h5>
                </div>
                <div class="carousel-item">
                    @*<img src="~/Content/images/Ella-Maise.jpg" alt="Ella Maise" />*@
                    <p>We found a caring and humble home tutor for our daughter. She likes studying with the home tutor. Great initiative.</p>
                    <h5> - Ella Maise</h5>
                </div>
            </div>
            <a class="carousel-control-prev" href="#testimonals" data-slide="prev">
                <span class="carousel-control-prev-icon"></span>
            </a>
            <a class="carousel-control-next" href="#testimonals" data-slide="next">
                <span class="carousel-control-next-icon"></span>
            </a>
        </div>
    </div>
</section>
<section class="call-toAction-sec subject-sec">
    <div class="container">
        <div class="row">
            <div class="col-12 col-md-12">
                <h4>Become an online tutor with Us</h4>
                <p class="mb-4 text-white">
                    Our online instant tutoring platform makes connecting with students or parents simple, convenient and flexible.
                    Our tutors are all passionate about their areas of expertise and eager to help students to learn.
                </p>
            </div>
            <div class="col-12 col-md-12 mb-4">
                <a href="@Url.Action("Tutor", "Account", new { area = "" })" hidefocus="hidefocus">Become a Tutor</a>
            </div>
        </div>
        <br/>
        <div class="row">
            <div class="col-12 col-md-12">
                <h4>Schedule A Session</h4>
                <p class="mb-4 text-white">
                    All Tutoring Sessions will be conducted by extremely qualified high school students on ZOOM. Zoom sessions can go up to 50 min. 
                    Students can bring their own material/questions to each Zoom session. We require that you sign up for Zoom Sessions 48 hours in advance.
                </p>
            </div>
            <div class="col-12 col-md-12">
                <a href="@Url.Action("Login", "Account", new { area = "" })" hidefocus="hidefocus">Schedule A Session</a>
            </div>
        </div>
    </div>
</section>


